{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "enabledManagers": ["docker", "golang", "custom"],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": ["^\\.devcontainer/Dockerfile$"],
      "matchStringsStrategy": "recursive",
      "matchStrings": ["ARG\\s+TYPESCRIPT_NODE_VERSION=(?<currentValue>\\S+)"],
      "datasourceTemplate": "docker",
      "depNameTemplate": "mcr.microsoft.com/devcontainers/typescript-node",
      "versioningTemplate": "docker"
    },
    {
      "customType": "regex",
      "managerFilePatterns": ["^\\.devcontainer/Dockerfile$"],
      "matchStringsStrategy": "recursive",
      "matchStrings": ["ARG\\s+DOCKER_GOLANG_VERSION=(?<currentValue>\\S+)"],
